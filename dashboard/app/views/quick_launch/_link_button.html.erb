<div class="<%= button_column_classes %>">
  <div id="<%= launchButtonId %>" class="launch-button">
    <a href="<%= launchButton[:url] || "#" %>" target="_blank">
      <div class="title text-center">cannon</div>
      <div class="panel-body panel-image">
        <img src="<%= asset_url launchButton[:logo] %>" alt="Application logo" width="<%= launchButton[:logoWidth] || 200 %>" class="center-block"  style="<%= launchButton[:style] || '' %>"/>
      </div>
      <div class="panel-body panel-text">
        <p><%= launchButton[:p1] || "&nbsp;".html_safe %></p>
        <p><%= launchButton[:p2] || "&nbsp;".html_safe %></p>
        <p><%= launchButton[:p3] || "&nbsp;".html_safe %></p>
      </div>
    </a>
  </div>
</div>

